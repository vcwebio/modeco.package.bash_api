#!/usr/bin/env bash

# START CONFIGURATION
export CONTECO_DC_GLOBAL_PACKAGE_CONTECO="bash.api"
export CONTECO_DC_GLOBAL_PACKAGE_TAG="5.0.18"
export CONTECO_DC_GLOBAL_STACKNAMES="bash_api"
export CONTECO_DC_GLOBAL_BOOTSTACKS="bash_api"
export CONTECO_DC_GLOBAL_RUNSTACKS="bash_api"
export CONTECO_DC_GLOBAL_VOLUME_LIST="${CONTECO_STACKPREFIX}_bash_api_volume,${CONTECO_STACKPREFIX}_bash_data_volume"
export CONTECO_DC_GLOBAL_VOLUME_INITIALISED="${CONTECO_STACKPREFIX}_bash_api_volume:/conteco/bin/bash,${CONTECO_STACKPREFIX}_bash_data_volume:/conteco/pwd"
# END CONFIGURATION

# START OTHER VARIABLES
export BASH_API_COMMAND="'--keepalive:0','.invoke hello'"
export BASH_API_COMMAND_INTERACTIVE="true"
export BASH_API_ENVIRONMENT=
# END OTHER VARIABLES

# START VOLUMES VARIABLES
export BASH_API_VOLUMES_MAPPINGS=""
export CONTECO_DC_GLOBAL_VOLUMES=": { }"
# END VOLUMES VARIABLES

# START PORTS VARIABLES
# END PORTS VARIABLES

# START NETWORKS VARIABLES
# END NETWORKS VARIABLES

# START DEPLOY VARIABLES
export BASH_API_DEPLOY_MODE="replicated"
export BASH_API_DEPLOY_RESOURCES_LIMITS_MEMORY="24m"
export BASH_API_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="5m"
# END DEPLOY VARIABLES
